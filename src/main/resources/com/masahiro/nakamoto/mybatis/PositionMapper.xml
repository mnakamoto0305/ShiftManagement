<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.masahiro.nakamoto.mybatis.PositionMapper">


  <!-- 指定したIDの拠点名を検索 -->
  <select id="findPosition" resultType="_int">
    SELECT
    position_id
    FROM
    users
    WHERE
    id = #{id}
  </select>

</mapper>